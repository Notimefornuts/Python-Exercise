


animals = { 'a': ['aardvark'], 'b': ['baboon'], 'c': ['coati']}

animals['d'] = ['donkey']
animals['d'].append('dog')
animals['d'].append('dingo')

print animals.values()
print len(animals.keys())
def howMany(aDict):
    count=0
    for i in aDict.keys():
        
        count=count+len(aDict[i])
    print count
howMany(animals)
